{
  "env": {
      "browser": true,
      "node": true,
	  "jest": true
   },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
	"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
	"react/prop-types": 0,
	"no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }]
  }
}